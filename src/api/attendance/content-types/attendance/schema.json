{
  "kind": "collectionType",
  "collectionName": "attendances",
  "info": {
    "singularName": "attendance",
    "pluralName": "attendances",
    "displayName": "Attendance"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "date": {
      "type": "date",
      "required": false
    },
    "open": {
      "type": "boolean",
      "default": false
    },
    "timer": {
      "type": "integer",
      "default": 60,
      "max": 145,
      "min": 60
    },
    "course": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::course.course",
      "inversedBy": "attendances"
    },
    "students": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::student.student",
      "mappedBy": "attendances"
    }
  }
}
